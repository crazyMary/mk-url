  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crazyMary/mk-url"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-transform-modules-commonjs": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "chai": "^4.2.0",
    "mocha": "^6.2.2"
  },
  "dependencies": {
    "core-js": "^3.4.1"
  },
  "engines": {
    "node": ">=8.9.0"
